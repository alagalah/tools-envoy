{
  "listeners": [
  {
    "address": "tcp://127.0.0.1:22001",
    "filters": [
      { "type": "read", "name": "tcp_proxy",
        "config": {
          "stat_prefix": "test_tcp",
          "route_config": {
            "routes": [
              {
                "cluster": "cluster_1"
              }
            ]
          },
          "access_log": [
            {
              "path": "/tmp/client-envoy-server.log"
            }
          ]
        }
      }
    ]
  }],
  "admin": { "access_log_path": "/dev/null", "address": "tcp://127.0.0.1:18125" },
  "statsd_udp_ip_address": "127.0.0.1:8125",

  "cluster_manager": {
    "clusters": [
    {
      "name": "cluster_1",
      "connect_timeout_ms": 5000,
      "type": "static",
      "lb_type": "round_robin",
      "hosts": [{"url": "tcp://127.0.0.1:22000"}]
    }]
  }
}
